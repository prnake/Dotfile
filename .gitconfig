[user]
	name = Papersnake
	email = prnake@gmail.com
	signingkey = prnake@gmail.com
[alias]
	st = status
[commit]
	gpgsign = false
[gpg]
	program = gpg
[pull]
	rebase = false
[init]
	defaultBranch = main
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
[url "git@git.tsinghua.edu.cn:"]
    insteadOf = https://git.tsinghua.edu.cn/
